{
	"name": "DS_archive_from_temp",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"fileName": {
				"type": "string"
			},
			"csvName": {
				"type": "string"
			}
		},
		"folder": {
			"name": "SalesData_archife_transform"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().csvName, '.csv')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().fileName, '/', substring(dataset().fileName, 0, 17))",
					"type": "Expression"
				},
				"fileSystem": "descontainer"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "first_name",
				"type": "String"
			},
			{
