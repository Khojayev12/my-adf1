				"type": "String"
			},
			{
				"name": "phone",
				"type": "String"
			},
			{
				"name": "address",
				"type": "String"
			},
			{
				"name": "gender",
				"type": "String"
			},
			{
				"name": "age",
				"type": "String"
			},
			{
				"name": "registered",
				"type": "String"
			},
			{
				"name": "orders",
				"type": "String"
			},
			{
